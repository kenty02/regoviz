{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:mocked": "vite",
    "dev": "cross-env NODE_ENV=development-nomock vite",
    "build": "tsc && vite build",
    "lint": "npx @biomejs/biome lint .",
    "format": "npx @biomejs/biome format .",
    "check": "npx @biomejs/biome check .",
    "check:apply": "npx @biomejs/biome check --apply .",
    "check:apply-unsafe": "npx @biomejs/biome check --apply-unsafe .",
    "preview": "vite preview",
    "generate": "orval",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "dependencies": {
    "@microlink/react-json-view": "^1.23.0",
    "@tanstack/react-query": "^5.14.2",
    "axios": "^1.6.2",
    "jotai": "^2.6.0",
    "react": "^18.2.0",
    "react-digraph": "^9.1.2",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@faker-js/faker": "^8.3.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "husky": "^8.0.0",
    "msw": "^2.0.11",
    "orval": "^6.23.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "overrides": {
    "html-react-parser": "^5.0.7"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
